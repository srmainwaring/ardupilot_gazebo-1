    <!-- source of gravity -->
    <model name="earth">
      <static>0</static>
      <link name="earth">
        <gravity>0</gravity>
        <!-- expo = -2.0 -->
        <inertial>
          <mass>1.50E+13</mass>
          <inertia>
            <ixx>5.99314E+14</ixx>
            <ixy>0</ixy>
            <ixz>0</ixz>
            <iyy>5.99314E+14</iyy>
            <iyz>0</iyz>
            <izz>5.99314E+14</izz>
          </inertia>
        </inertial>
        <!-- expo = -0.5 -->
        <!-- <inertial>
          <mass>4.74E+11</mass>
          <inertia>
            <ixx>1.89520E+13</ixx>
            <ixy>0</ixy>
            <ixz>0</ixz>
            <iyy>1.89520E+13</iyy>
            <iyz>0</iyz>
            <izz>1.89520E+13</izz>
          </inertia>
        </inertial> -->
        <collision name="collision">
          <pose degrees="true">0 0 0 90 0 -90</pose>
          <geometry>
            <sphere>
              <radius>10</radius>
            </sphere>
          </geometry>
          <surface>
            <friction>
              <ode>
                <mu>1.0</mu>
                <mu2>1.0</mu2>
              </ode>
            </friction>
          </surface>
          </collision>
        <visual name="visual">
          <pose degrees="true">0 0 0 90 0 -90</pose>
          <geometry>
            <sphere>
              <radius>10</radius>
            </sphere>
          </geometry>
          <material>
            <ambient>1 1 1</ambient>
            <diffuse>1 1 1</diffuse>
            <specular>1 1 1</specular>
            <pbr>
              <metal>
                  <albedo_map>ear0xuu2.jpg</albedo_map>
                  <metalness>0.1</metalness>
              </metal>
            </pbr>
          </material>
        </visual>
      </link>

      <plugin filename="gz-sim-velocity-control-system"
        name="gz::sim::systems::VelocityControl">
        <initial_angular>0 0 0.0</initial_angular>
      </plugin>

    </model>

    <!-- red satellite in equatorial orbit -->
    <model name="satellite1">
      <pose>-15 0 0 0 0 0</pose>
      <link name="base_link">
        <gravity>1</gravity>
        <inertial>
          <mass>1.0</mass>
          <inertia>
            <ixx>0.1</ixx>
            <ixy>0</ixy>
            <ixz>0</ixz>
            <iyy>0.1</iyy>
            <iyz>0</iyz>
            <izz>0.1</izz>
          </inertia>
        </inertial>
        <collision name="collision">
          <pose degrees="true">0 0 0 90 0 0</pose>
          <geometry>
            <sphere>
              <radius>0.5</radius>
            </sphere>
          </geometry>
        </collision>
        <visual name="visual">
          <pose degrees="true">0 0 0 90 0 0</pose>
          <geometry>
            <sphere>
              <radius>0.5</radius>
            </sphere>
          </geometry>
          <material>
            <ambient>1.0 0.0 0.0</ambient>
            <diffuse>1.0 0.0 0.0</diffuse>
            <specular>0.01 0.01 0.01 1.0</specular>
          </material>
        </visual>
      </link>

      <plugin filename="InitialVelocityPlugin"
        name="InitialVelocityPlugin">
        <debug>0</debug>
        <linear>0 8.164965809 0</linear>
        <link>base_link</link>
        <p_gain>50</p_gain>
        <i_gain>1</i_gain>
        <d_gain>0</d_gain>
        <i_max>10</i_max>
        <i_min>-10</i_min>
        <cmd_min>-1000</cmd_min>
        <cmd_max>1000</cmd_max>
      </plugin>
    </model>

    <!-- blue satellite in polar orbit -->
    <model name="satellite2">
      <pose>0 0 20 0 0 0</pose>
      <link name="base_link">
        <gravity>1</gravity>
        <inertial>
